    "            kg_confs.extend(    confs['kg_conf'].view(-1).tolist() )\n",
    "\n",
    "    # apply best threshold\n",
    "    final_preds = [1 if p > best_thr_for_split else 0 for p in final_probs]\n",
    "\n",
